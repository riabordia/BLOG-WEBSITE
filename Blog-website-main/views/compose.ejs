<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input id="title" class="form-control" type="text" name="newPostTitle">
    </div>
    <div class="form-group">
    <label for="postData">Post</label>
    <textarea name="newPostText" class="form-control" id="postData" rows="4" cols="80"></textarea>

  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>

</form>
<%- include("partials/footer") -%>
